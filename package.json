{
  "name": "naijafr",
  "version": "1.0.0",
  "description": "E-commerce Platform connecting France to Naija built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mf4z/NaijaFR.git"
  },
  "keywords": [
    "MERN",
    "E-commerce",
    "Naija",
    "FR"
  ],
  "author": "Marwan Mai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mf4z/NaijaFR/issues"
  },
  "homepage": "https://github.com/Mf4z/NaijaFR#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "mongoose": "^8.4.1"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.3"
  }
}
